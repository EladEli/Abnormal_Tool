<Window x:Class="Abnormal_UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Test Toolbox" Height="550" Width="950">
    <Grid>
        <TabControl TabStripPlacement="Left">
            <TabItem Header="Abnormal">
                <ContentControl Content="{Binding _abnormalAttackWindow, Mode=OneWay}"/>
            </TabItem>

            <TabItem Header="LSB">
                <ContentControl Content="{Binding _lsbAttackWindow, Mode=OneWay}"/>
            </TabItem>

            <TabItem Header="Test" Height="24" VerticalAlignment="Top">
                <ContentControl Content="{Binding _testWindow, Mode=OneWay}"/>
            </TabItem>
            <TabItem Header="Dsa" Margin="-2,-2,0,-2">
                <ContentControl Content="{Binding _dsaWindow, Mode=OneWay}"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
